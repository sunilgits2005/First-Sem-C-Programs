#include <stdio.h>
int main()
{
int n, a=0, b=1, c;
printf("Enter the number of terms in Fibonacci series: ");
scanf("%d", &n);
if(n <= 0)
{
printf("Number of terms must be positive.\n");
return 1;
}
printf("%d ", a);
if(n > 1) printf("%d ", b);
for(int i=3; i<=n; i++) {
c = a + b;
printf("%d ", c);
a = b;
b = c;
}
printf("\n");
return 0;
}
